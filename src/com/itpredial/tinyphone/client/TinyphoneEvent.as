package com.itpredial.tinyphone.client{		public class TinyphoneEvent	{		var id_:String;		var callerNumber:String;		var eventType:String;		var value_:String;		public function TinyphoneEvent()		{			// empty		}				public function getId():String		{			return id_;		}				public function setId(id:String):void		{			id_ = id;		}				public function getCallerNumber():String		{			return callerNumber;		}				public function setCallerNumber(callerNumber:String):void		{			this.callerNumber = callerNumber;		}				public function getEventType():String		{			return eventType;		}				public function setEventType(eventType:String):void		{			if (eventType == "new_call")			{				this.eventType = EventType.NEW_CALLER;			}			else if (eventType == "keypress")			{				this.eventType = EventType.KEYPRESS;			}			else if (eventType == "audio_level")			{				this.eventType = EventType.AUDIO_LEVEL;			}			else if (eventType == "hangup")			{				this.eventType = EventType.HANGUP;			}		}				public function typeEquals(type:String):Boolean		{			return (eventType == type);		}				public function getValue():String		{			return value_;		}				public function setValue(value_:String):void		{			this.value_ = value_;		}				public function toString():String		{			var str:String = "Tinyphone Event " + eventType + ": id=" + id_;						if (eventType == EventType.NEW_CALLER)			{				str += ", caller Number=" + callerNumber;			}			else			{				str += ", value=" + value_;			}						return str;		}	}}